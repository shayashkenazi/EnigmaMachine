<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane fx:id="sp_loginPage" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.LoginController">
   <content>
      <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="700.0">
         <children>
            <Label text="Log In">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="229.0" prefWidth="700.0" spacing="20.0">
               <children>
                  <Label text="User Name:" />
                  <TextField fx:id="tf_userName" prefHeight="32.0" prefWidth="223.0" />
               </children>
            </HBox>
            <Button fx:id="btn_login" defaultButton="true" mnemonicParsing="false" onAction="#loginBtnClick" text="login" />
         </children>
      </VBox>
   </content>
</ScrollPane>
